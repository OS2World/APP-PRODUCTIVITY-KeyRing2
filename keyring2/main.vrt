    IF Ok = 1 THEN DO
        DO I = 1 TO Windows.0
            W = WORD(Windows.i, 1)
            IF Vrget(W, "Visible") = 1 THEN DO
                IF POS( "Netscape", Vrget(W, "Caption"), 1 ) = 1 THEN DO
                    RETURN W
